{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["off"],
    "comma-dangle": ["off"],
    "operator-linebreak": ["off"],
    "react/jsx-one-expression-per-line": ["off"],
    "react/jsx-curly-brace-presence": ["off"],
    "no-underscore-dangle": "off"
  }
}
